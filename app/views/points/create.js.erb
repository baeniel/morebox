<% if @result %>
  swal({
    title: "<%= @title %>",
    button: false,
    text: "<%= @body %>"
  });
  setTimeout(function(){
    window.location = '/items/auto_out'
  }, 2000);
<% else %>
  swal({
    title: "<%= @title %>",
    text: "<%= @body %>",
    type: "error",
    confirmButtonText: '확인',
    confirmButtonColor: "#200e57"
  });
<% end %>
